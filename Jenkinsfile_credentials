node{

    agent any

    options { disableConcurrentBuilds() }

        stages{
            stage("Jenkins Credentials"){
                steps{
                    withCredentials([usernamePassword(credentialsId: 'Admin'),
                                                    passwordVariable: 'password'
                                                    usernameVariable: 'username')] {
                    creds = "\nUsername: ${username} \nPassword: ${password}\n"
                    }
                    println creds
                }
            }

            stage("Build"){

                steps{

                    echo "This is the Build step"

                    bat "PING -n 10 127.0.0.1>nul"

                }

            }

            stage("Test"){

                steps{

                    echo "This is the Test step"

                }

            }

            stage("QA"){

                steps{

                    echo "This is the QA step"

                }

            }

            stage("Deploy"){

                steps{

                    echo "This is the Deploy step"

                }

            }

        }

}